generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type HighlightsAllHighlights {
  highlights HighlightsAllHighlightsHighlights[]
  source     String
}

type HighlightsAllHighlightsHighlights {
  comment  HighlightsAllHighlightsHighlightsComment
  content  HighlightsAllHighlightsHighlightsContent
  id       String
  position HighlightsAllHighlightsHighlightsPosition
}

type HighlightsAllHighlightsHighlightsComment {
  emoji String
  text  String
}

type HighlightsAllHighlightsHighlightsContent {
  image String?
  text  String?
}

type HighlightsAllHighlightsHighlightsPosition {
  boundingRect HighlightsAllHighlightsHighlightsPositionBoundingRect
  pageNumber   Int
  rects        HighlightsAllHighlightsHighlightsPositionRects[]
}

type HighlightsAllHighlightsHighlightsPositionBoundingRect {
  /// Multiple data types found: Float: 12.5%, Int: 87.5% out of 8 sampled entries
  height     Json
  pageNumber Int?
  width      Float
  x1         Float
  x2         Float
  /// Multiple data types found: Float: 75%, Int: 25% out of 8 sampled entries
  y1         Json
  /// Multiple data types found: Float: 75%, Int: 25% out of 8 sampled entries
  y2         Json
}

type HighlightsAllHighlightsHighlightsPositionRects {
  /// Multiple data types found: Float: 7.1%, Int: 92.9% out of 14 sampled entries
  height     Json
  pageNumber Int?
  width      Float
  x1         Float
  x2         Float
  y1         Float
  y2         Float
}

model highlights {
  id            String                    @id @default(auto()) @map("_id") @db.ObjectId
  allHighlights HighlightsAllHighlights[]
  user          String
}
